{
  "name": "library-webpack-base",
  "version": "0.0.1",
  "description": "For authoring libraries.",
  "main": "src/index.js",
  "scripts": {
    "test": "      echo \"Error: no test specified\" && exit 1",
    "build": "     webpack            --config webpack.prod.js --mode production  --env production  --progress --color",
    "build-dev": " webpack            --config webpack.dev.js  --mode development --env development --progress --color",
    "gzip": "      find dist -type f -regex '.*\\(html\\|js\\|map\\|css\\)$' -exec gzip   -fk9 {} \\;",
    "brotli": "    find dist -type f -regex '.*\\(html\\|js\\|map\\|css\\)$' -exec brotli -fk9 {} \\;",
    "serve": "     node_modules/node-static/bin/cli.js -a 0.0.0.0 --spa -p 3000 ./dist",
    "serve-dev": " node_modules/node-static/bin/cli.js -a 0.0.0.0 --spa -p 8081 ./dev",
    "serve-test": "node_modules/node-static/bin/cli.js -a 0.0.0.0 --spa -p 8080 ./test"
  },
  "repository": "git+ssh://git@github.com/austinjp/library-webpack-base.git",
  "author": "austinjp",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.14.3",
    "@babel/plugin-transform-react-jsx-development": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "node-static": "^0.7.11",
    "path": "^0.12.7",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0"
  }
}
